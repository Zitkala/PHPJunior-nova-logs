{
    "name": "zitkala/php-junior-nova-logs",
    "description": "Provides a Log Viewer for Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nyi Nyi Lwin",
            "email": "nyinyilwin1992@hotmail.com"
        },
        {
            "name":     "Norman Huth",
            "email":    "norman@huth.it",
            "homepage": "https://huth.it",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "zitkala/arcanedev-log-viewer": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "7.1"
    },
    "autoload": {
        "psr-4": {
            "Zitkala\\NovaLogViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zitkala\\NovaLogViewer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zitkala\\NovaLogViewer\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
